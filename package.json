{
  "name": "@amnis/state",
  "version": "0.15.3",
  "description": "Core data managers, state reducers, and common interfaces for web applications.",
  "keywords": [],
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/amnis-dev/amnis-state"
  },
  "homepage": "https://github.com/amnis-dev/amnis-state",
  "author": "Eric Crowell <admin@amnis.dev>",
  "files": [
    "types",
    "dist"
  ],
  "types": "./types/index.d.ts",
  "main": "./dist/index.umd.cjs",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./schema": {
      "types": "./types/schema/index.d.ts",
      "import": "./dist/schema.js",
      "require": "./dist/schema.cjs"
    },
    "./context": {
      "types": "./types/context/index.d.ts",
      "import": "./dist/context.js",
      "require": "./dist/context.cjs"
    }
  },
  "scripts": {
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "build": "pnpm build:types && pnpm build:dist",
    "build:dist": "vite build --mode production",
    "build:types": "tsc -p tsconfig.build.json",
    "test": "vitest --run",
    "gen:schema": "ts-node-esm -P scripts/tsconfig.json scripts/schema",
    "lint": "tsc --noEmit && eslint --ext .ts --ext .tsx \"./\"",
    "clean": "rimraf ./dist ./lib ./types ./tsconfig.build.tsbuildinfo ./tsconfig.tsbuildinfo"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "*"
  },
  "devDependencies": {
    "@amnis/eslint-config-node": "^2.2.4",
    "@amnis/tsconfig-node": "^2.2.4",
    "@reduxjs/toolkit": "alpha",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.0.1",
    "@types/node": "^18.14.0",
    "ajv": "^8.12.0",
    "cross-fetch": "^3.1.5",
    "eslint": "^8.34.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^11.1.0",
    "glob": "^8.1.0",
    "rimraf": "^4.1.2",
    "standard-version": "^9.5.0",
    "ts-json-schema-generator": "^1.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.3",
    "vitest": "^0.28.5"
  },
  "sideEffects": false,
  "pnpm": {
    "patchedDependencies": {
      "redux@5.0.0-alpha.2": "patches/redux@5.0.0-alpha.2.patch",
      "redux-thunk@3.0.0-alpha.1": "patches/redux-thunk@3.0.0-alpha.1.patch",
      "@reduxjs/toolkit@2.0.0-alpha.2": "patches/@reduxjs__toolkit@2.0.0-alpha.2.patch",
      "immer@9.0.19": "patches/immer@9.0.19.patch",
      "reselect@4.1.7": "patches/reselect@4.1.7.patch"
    }
  }
}
