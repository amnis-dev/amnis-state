{
  "name": "@amnis/state",
  "version": "0.13.1",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/amnis-dev/amnis-state"
  },
  "homepage": "https://github.com/amnis-dev/amnis-state",
  "files": [
    "lib",
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.umd.cjs",
      "import": "./dist/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "scripts": {
    "build": "pnpm build:lib && pnpm build:dist",
    "build:dist": "vite build",
    "build:lib": "tsc -p tsconfig.build.json",
    "lint": "tsc --noEmit && eslint --ext .ts --ext .tsx \"./\"",
    "clean": "rimraf ./dist ./lib ./types ./tsconfig.build.tsbuildinfo"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "*"
  },
  "devDependencies": {
    "@amnis/eslint-config-node": "^2.2.1",
    "@amnis/tsconfig-node": "^2.2.1",
    "@reduxjs/toolkit": "alpha",
    "@types/node": "^18.13.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "rimraf": "^4.1.2",
    "ts-json-schema-generator": "^1.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.2",
    "vitest": "^0.28.5"
  },
  "sideEffects": false
}
