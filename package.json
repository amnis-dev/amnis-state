{
  "name": "@amnis/state",
  "version": "0.13.1",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/amnis-dev/amnis-state"
  },
  "homepage": "https://github.com/amnis-dev/amnis-state",
  "files": [
    "types",
    "dist"
  ],
  "types": "./types/index.d.ts",
  "main": "./dist/index.umd.cjs",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "build": "pnpm build:types && pnpm build:dist",
    "build:dist": "vite build --mode production",
    "build:types": "tsc -p tsconfig.build.json",
    "test": "vitest --run",
    "gen:schema": "ts-node-esm -P scripts/tsconfig.json scripts/schema",
    "lint": "tsc --noEmit && eslint --ext .ts --ext .tsx \"./\"",
    "clean": "rimraf ./dist ./lib ./types ./tsconfig.build.tsbuildinfo"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "*"
  },
  "devDependencies": {
    "@amnis/eslint-config-node": "^2.2.2",
    "@amnis/tsconfig-node": "^2.2.2",
    "@reduxjs/toolkit": "alpha",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.0.1",
    "@types/node": "^18.13.0",
    "eslint": "^8.34.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^11.1.0",
    "glob": "^8.1.0",
    "rimraf": "^4.1.2",
    "ts-json-schema-generator": "^1.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.2",
    "vitest": "^0.28.5"
  },
  "sideEffects": false
}
